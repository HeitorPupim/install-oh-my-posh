{
   "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
   "blocks": [
     {
       "alignment": "left",
       "segments": [
         {
           "background": "#b300ff",
           "foreground": "#ffffff",
           "style": "plain",
           "template": " \uf64f {{ .CurrentDate | date .Format }} ",
           "type": "time"
         },
         {
           "background": "#b300ff",
           "foreground": "#ffffff",
           "properties": {
             "always_enabled": true
           },
           "style": "plain",
           "template": " \ufa1e{{ .FormattedMs }} ",
           "type": "executiontime"
         }
       ],
       "type": "rprompt"
     },
     {
       "alignment": "left",
       "segments": [
         {
           "background": "#860082",
           "foreground": "#ffffff",
           "properties": {
             "display_host": false
           },
           "style": "diamond",
           "template": "{{if .Root}} \uf0e7 {{.UserName}} {{else}} {{.UserName}} {{end}}",
           "trailing_diamond": "\ue0b0",
           "type": "session"
         },
         {
           "background": "#2fff00",
           "foreground": "#000000",
           "powerline_symbol": "\ue0b0",
           "properties": {
             "folder_icon": "\uf115",
             "folder_separator_icon": " \ue0b1 ",
             "max_depth": 2,
             "style": "agnoster_short"
           },
           "style": "powerline",
           "template": " {{ .Path }} ",
           "type": "path"
         },
         {
           "background": "#95ffa4",
           "foreground": "#193549",
           "powerline_symbol": "\ue0b0",
           "style": "powerline",
           "template": " {{ .HEAD }} ",
           "type": "git"
         }
       ],
       "type": "prompt"
     }
   ],
   "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.UserName}} \u2794 üìÅ{{.Folder}}",
   "final_space": true,
   "version": 2
 }
 